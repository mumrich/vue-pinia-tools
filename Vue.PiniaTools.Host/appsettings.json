{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SinglePageApps": {
    "/child-app": {
      "DevServerAddress": "http://localhost:8989/child-app/",
      "SpaRootPath": "App/child-app",
      "NodePackageManager": "Yarn",
      "NodeStartScript": "dev",
      "Regex": "dev server running at:",
      "Bundler": "Custom",
      "Environment": {
        "HMR_PORT": "%ASPNETCORE_HTTPS_PORT%",
        "HMR_PROTOCOL": "wss",
        "SERVER_BASE": "/child-app/"
      },
      "CustomYarpConfiguration": {
        "Routes": {
          "spa-client-app": {
            "ClusterId": "client-app-cluster",
            "Match": "/client-app/{**any}"
          }
        }
      }
      //"SpaAssetsExpression": ".*\/child-app\/(src|node_modules|@[a-zA-Z]+|.*vite.*).*" // ^(src|node_modules|@[a-zA-Z]+|.*vite.*)$
    } //,
    // "/": {
    //   "DevServerAddress": "http://localhost:3000/",
    //   "SpaRootPath": "App/mumrich-vue-pinia-tools",
    //   "NodePackageManager": "Yarn",
    //   "NodeStartScript": "dev",
    //   "Regex": "dev server running at:",
    //   "Bundler": "ViteJs",
    //   "Environment": {
    //     "HMR_PORT": "%ASPNETCORE_HTTPS_PORT%",
    //     "HMR_PROTOCOL": "wss"
    //   }
    // }
  },
  "ReverseProxy": {
    "Routes": {
      "child-app-index": {
        "ClusterId": "child-app-index",
        "Match": {
          "Path": "/child-app/"
        }
      },
      "child-app-assets": {
        "ClusterId": "child-app-assets",
        "Match": {
          "Path": "/child-app/{**any}"
        }
      }
    },
    "Clusters": {
      "child-app-index": {
        "Destinations": {
          "child-app-index": {
            "Address": "http://localhost:8989/child-app/"
          }
        }
      },
      "child-app-assets": {
        "Destinations": {
          "child-app-assets": {
            "Address": "http://localhost:8989/"
          }
        }
      }
    }
  }
}